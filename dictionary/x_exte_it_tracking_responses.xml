<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Responses" max_length="40" name="x_exte_it_tracking_responses" type="collection">
        <element label="Assignee" max_length="32" name="assignee" reference="sys_user" type="reference"/>
        <element label="Assigner" max_length="32" name="assigner" read_only="true" reference="sys_user" type="reference"/>
        <element label="Due date" max_length="40" name="due_date" type="glide_date"/>
        <element label="Related issue" max_length="32" name="issue" reference="x_exte_it_tracking_issues" type="reference"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" display="true" label="Number" max_length="40" name="number" type="string"/>
        <element choice="1" label="Priority" max_length="40" name="priority" type="choice">
            <choice>
                <element label="Critical" sequence="0" value="1"/>
                <element label="High" sequence="1" value="2"/>
                <element label="Medium" sequence="2" value="3"/>
                <element label="Standard" sequence="3" value="4"/>
                <element label="Planning" sequence="4" value="5"/>
            </choice>
        </element>
        <element label="Response" max_length="1000" name="response" type="string"/>
        <element choice="1" label="Risk level" max_length="40" name="risk_level" type="choice">
            <choice/>
        </element>
        <index name="index">
            <element name="issue"/>
        </index>
        <index name="index2">
            <element name="assigner"/>
        </index>
        <index name="index3">
            <element name="assignee"/>
        </index>
    </element>
</database>
